{
    "manifest_version": 3,
    "name": "YouTube Quick Search",
    "version": "1.0",
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "service_worker": "service-worker.js"       
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "16": "icon16.png",
        "128": "icon128.png"
    }
}
